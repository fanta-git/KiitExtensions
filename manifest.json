{
    "name": "Kiite-Expantion",
    "author": "fanta",
    "description": "Kiite",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": [
		"tabs",
        "storage"
    ],
    "background":{
        "scripts":[
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["https://cafe.kiite.jp/*"],
            "css": ["CafeExp/style.css"],
            "js" : ["CafeExp/script.js"]
        },
        {
            "matches": ["https://embed.nicovideo.jp/watch/*"],
            "js" : ["CafeExp/iframe.js"],
            "all_frames": true
        }
    ]
}